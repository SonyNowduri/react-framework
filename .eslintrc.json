{
        "env": {
            "browser": true,
            "es2021": true,
            "node": true
        },
        "parser":"babel-eslint",
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended"
        ],
        "parserOptions": {
            "ecmaFeatures": {
                "jsx": true
            },
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "plugins": [
            "react"
        ],
        "rules": {
            "no-console": 0,
            "no-unused-vars": 0,
            "react/prop-types": "off",
            "space-before-function-paren": ["error", {
                "anonymous": "always",
                "named": "never",
                "asyncArrow": "always"
            }],
            "multiline-ternary": "off",
            "no-use-before-define": "off"
        }
    
    
}